// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Users {
  id       String @id @default(cuid())
  rank     Int    @default(1)
  fistName String
  lastName String
  imageUrl String
}
